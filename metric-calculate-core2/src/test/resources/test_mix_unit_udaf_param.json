{
  "aggregateType": "BASEMIX",
  "mixAggMap": {
    "上海_sum": {
      "aggregateType": "SUM",
      "metricExpressParam": {
        "express": "city == '上海' ? amount : 0"
      }
    },
    "全国_sum": {
      "aggregateType": "SUM",
      "metricExpressParam": {
        "express": "amount"
      }
    }
  },
  "metricExpressParam": {
    "express": "上海_sum / 全国_sum"
  }
}